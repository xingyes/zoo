<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <!-- 纯色 -->
            <solid android:color="#E1E1E1"/>
            <!-- 描边 -->
            <stroke
                android:color="#D0D0D0"
                android:width="1dp"/>
            <!-- 圆角 -->
            <corners android:radius="25dp"/>
        </shape>
    </item>
</selector>