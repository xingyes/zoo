<?xml version="1.0" encoding="utf-8" ?>
<resources>
    <declare-styleable name="CarouselFigureViewPager">
    <attr name="hdpi_len" format="dimension"/>
        <attr name="isCarousel" format="boolean"/>

    </declare-styleable>


    <declare-styleable name="NaviBar">
        <attr name="bgcolor" format="color"/>

        <attr name="titleText" format="string"/>
        <attr name="titleTextSize" format="dimension"/>
        <attr name="titleTextColor" format="color"/>
        <attr name="titleTextStyle" format="reference"/>
        <attr name="titleTextImg" format="reference"/>

        <attr name="leftText" format="string"/>
        <attr name="leftVisible" format="boolean"/>

        <attr name="rightText" format="string"/>
        <attr name="rightImg" format="reference"/>

        <attr name="rightTextSize" format="dimension"/>
        <attr name="rightTextColor" format="color"/>
        <attr name="rightTextStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WheelPicker">
        <attr name="startYear" format="integer|reference"/>
        <attr name="endYear" format="integer|reference"/>
        <attr name="referWidth" format="integer"/>
        <attr name="referHeight" format="integer"/>
    </declare-styleable>


    <declare-styleable name="AbstractWheelView">
        <attr name="visibleItems" format="integer"/>
        <attr name="isAllVisible" format="boolean"/>
        <attr name="itemOffsetPercent" format="integer"/>
        <attr name="itemsPadding" format="dimension"/>
        <attr name="selectionDividerDimmedAlpha" format="integer"/>
        <attr name="selectionDividerActiveAlpha" format="integer"/>
        <attr name="selectionDivider" format="reference"/>
        <attr name="itemsDimmedAlpha" format="integer"/>
        <attr name="isCyclic" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="WheelVerticalView">
        <attr name="selectionDividerHeight" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="WheelHorizontalView">
        <attr name="selectionDividerWidth" format="dimension"/>
    </declare-styleable>
    <attr name="abstractWheelViewStyle" format="reference" />


    <item name="BaseQuickAdapter_viewholder_support" type="id"/>
    <item name="BaseQuickAdapter_swiping_support" type="id"/>
    <item name="BaseQuickAdapter_dragging_support" type="id"/>
    <item name="BaseQuickAdapter_databinding_support" type="id"/>


</resources>
